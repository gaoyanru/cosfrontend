:global
  @media only print
    .print
      font-family: '宋体'
      .title
        font-size: 16px
        text-align: center
        padding-bottom: 20px
      table
        width: 100%
        border-collapse: collapse
        td, th
          font-weight: normal
          text-align: center
          font-size: 14px
          border:1px solid #F00
          padding: 0px 8px
          color: red
